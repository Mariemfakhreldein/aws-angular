{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "aws-angular": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/aws-angular",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/images/avatar/1.jpg",
              "src/assets/images/avatar/1.png"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/style.css",
              "src/assets/icons/themify-icons/fonts/themfiy-icons",
              "src/assets/icons/weather-icons/css/weather-icons-min.css",
              "src/assets/icons/simple-line-icons/css/simple-line-icons.css",
              "src/assets/icons/pe-icon-set-weather/css/helper.css",
              "src/assets/icons/pe-icon-set-weather/css/pe-icon-set-weather.css",
              "node_modules/bootstrap/dist/css/bootstrap.css"
            ],
            "scripts": [
              "src/assets/js/dashboard/dashboard-1.js",
              "src/assets/js/plugins-init/bootstrap-duallistbox-init.js",
              "src/assets/js/plugins-init/jquery.bootgrid-init.js",
              "src/assets/js/plugins-init/jquery.flot.init.js",
              "src/assets/js/plugins-init/jquery.validate-init.js",
              "src/assets/js/plugins-init/jquery-asColorPicker.init.js",
              "src/assets/js/plugins-init/jquery-steps-init.js",
              "src/assets/js/plugins-init/amchart-init.js",
              "src/assets/js/plugins-init/block-ui-init.js",
              "src/assets/js/plugins-init/bootstrap-editable-init.js",
              "src/assets/js/plugins-init/bootstrap-multiselect-init.js",
              "src/assets/js/plugins-init/bootstrap-notify-init.js",
              "src/assets/js/plugins-init/bootstrap-select-init.js",
              "src/assets/js/plugins-init/bootstrap-tagsinput-init.js",
              "src/assets/js/plugins-init/bootstrap-touchpin-init.js",
              "src/assets/js/plugins-init/bs-date-picker-init.js",
              "src/assets/js/plugins-init/bs-daterange-picker-init.js",
              "src/assets/js/plugins-init/c3-init.js",
              "src/assets/js/plugins-init/chart-google-init.js",
              "src/assets/js/plugins-init/chartist.init.js",
              "src/assets/js/plugins-init/chartjs-init.js",
              "src/assets/js/plugins-init/chartjs-plugin-datalabels-init.js",
              "src/assets/js/plugins-init/chartjs-streaming-init.js",
              "src/assets/js/plugins-init/chartjs-widget-init.js",
              "src/assets/js/plugins-init/circle-invoice-init.js",
              "src/assets/js/plugins-init/circle-progress-init.js",
              "src/assets/js/plugins-init/clipboard-init.js",
              "src/assets/js/plugins-init/clock-picker-init.js",
              "src/assets/js/plugins-init/color-picker-init.js",
              "src/assets/js/plugins-init/counterup-init.js",
              "src/assets/js/plugins-init/cropperjs-init.js",
              "src/assets/js/plugins-init/croppie-init.js",
              "src/assets/js/plugins-init/datamap-init.js",
              "src/assets/js/plugins-init/datamap-usa-init.js",
              "src/assets/js/plugins-init/datamap-world-init.js",
              "src/assets/js/plugins-init/datatable-page-contact-init.js",
              "src/assets/js/plugins-init/datatables.init.js",
              "src/assets/js/plugins-init/datedropper-init.js",
              "src/assets/js/plugins-init/dragula-init.js",
              "src/assets/js/plugins-init/dropify-init.js",
              "src/assets/js/plugins-init/easy-pie-chart-init.js",
              "src/assets/js/plugins-init/echarts-init.js",
              "node_modules/bootstrap/dist/css/bootstrap.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "aws-angular:build:production"
            },
            "development": {
              "browserTarget": "aws-angular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "aws-angular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "aws-angular"
}
